{
    "Title": "Practice in UAT",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
              "Microsoft": "Information",
              "System": "Information"
        },
        "WriteTo": [
          {
            "Name": "File",
            "Args": {
              "path": "../Providers_UAT.log",
              "rollingInterval": "Day"
            }
          }
        ],
        "Enrich": [ "FromLogContext" ]
      }
    }
}