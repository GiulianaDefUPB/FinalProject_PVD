{
    "Title": "Practice in QA",
    "Serilog": {
      "MinimumLevel": {
        "Default": "Warning",
        "Override": {
          "Microsoft": "Information",
          "System": "Information"
        },
        "WriteTo": [
          {
            "Name": "File",
            "Args": {
              "path": "../Providers_QA.log",
              "rollingInterval": "Day"
            }
          }
        ],
        "Enrich": [ "FromLogContext" ]
      }
    }
}